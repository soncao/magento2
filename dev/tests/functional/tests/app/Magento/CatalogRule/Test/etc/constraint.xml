<constraint>
    <assertCatalogPriceRuleSuccessSaveMessage module="Magento_CatalogRule">
        <severity>low</severity>
        <require>
            <pageCatalogRuleIndex class="Magento\CatalogRule\Test\Page\Adminhtml\CatalogRuleIndex"/>
        </require>
    </assertCatalogPriceRuleSuccessSaveMessage>
    <assertCatalogPriceRuleForm module="Magento_CatalogRule">
        <severity>low</severity>
        <require>
            <catalogPriceRule class="Magento\CatalogRule\Test\Fixture\CatalogRule"/>
            <pageCatalogRuleIndex class="Magento\CatalogRule\Test\Page\Adminhtml\CatalogRuleIndex"/>
            <pageCatalogRuleNew class="Magento\CatalogRule\Test\Page\Adminhtml\CatalogRuleNew"/>
        </require>
    </assertCatalogPriceRuleForm>
    <assertCatalogPriceRuleInGrid module="Magento_CatalogRule">
        <severity>low</severity>
        <require>
            <catalogPriceRule class="Magento\CatalogRule\Test\Fixture\CatalogRule"/>
            <pageCatalogRuleIndex class="Magento\CatalogRule\Test\Page\Adminhtml\CatalogRuleIndex"/>
        </require>
    </assertCatalogPriceRuleInGrid>
    <assertCatalogPriceRuleNoticeMessage module="Magento_CatalogRule">
        <severity>low</severity>
        <require>
            <pageCatalogRuleIndex class="Magento\CatalogRule\Test\Page\Adminhtml\CatalogRuleIndex"/>
        </require>
    </assertCatalogPriceRuleNoticeMessage>
    <assertCatalogPriceRuleAppliedProductPage module="Magento_CatalogRule">
        <severity>high</severity>
        <require>
            <cmsIndex class="Magento\Cms\Test\Page\CmsIndex"/>
            <product class="Magento\Catalog\Test\Fixture\CatalogProductSimple" />
            <pageCatalogProductView class="Magento\Catalog\Test\Page\Product\CatalogProductView"/>
            <catalogCategoryView class="Magento\Catalog\Test\Page\Category\CatalogCategoryView"/>
        </require>
    </assertCatalogPriceRuleAppliedProductPage>
    <assertCatalogPriceRuleSuccessDeleteMessage module="Magento_CatalogRule">
        <severity>low</severity>
        <require>
            <pageCatalogRuleIndex class="Magento\CatalogRule\Test\Page\Adminhtml\CatalogRuleIndex"/>
        </require>
    </assertCatalogPriceRuleSuccessDeleteMessage>
    <assertCatalogPriceRuleNotInGrid module="Magento_CatalogRule">
        <severity>low</severity>
        <require>
            <catalogPriceRule class="Magento\CatalogRule\Test\Fixture\CatalogRule"/>
            <pageCatalogRuleIndex class="Magento\CatalogRule\Test\Page\Adminhtml\CatalogRuleIndex"/>
        </require>
    </assertCatalogPriceRuleNotInGrid>
    <assertCatalogPriceRuleAppliedCatalogPage module="Magento_CatalogRule">
        <severity>high</severity>
        <require>
            <cmsIndex class="Magento\Cms\Test\Page\CmsIndex"/>
            <product class="Magento\Catalog\Test\Fixture\CatalogProductSimple" />
            <catalogCategoryView class="Magento\Catalog\Test\Page\Category\CatalogCategoryView"/>
        </require>
    </assertCatalogPriceRuleAppliedCatalogPage>
    <assertCatalogPriceRuleAppliedShoppingCart module="Magento_CatalogRule">
        <severity>high</severity>
        <require>
            <cmsIndex class="Magento\Cms\Test\Page\CmsIndex"/>
            <product class="Magento\Catalog\Test\Fixture\CatalogProductSimple" />
            <pageCatalogProductView class="Magento\Catalog\Test\Page\Product\CatalogProductView"/>
            <catalogCategoryView class="Magento\Catalog\Test\Page\Category\CatalogCategoryView"/>
            <pageCheckoutCart class="Magento\Checkout\Test\Page\CheckoutCart"/>
        </require>
    </assertCatalogPriceRuleAppliedShoppingCart>
</constraint>
